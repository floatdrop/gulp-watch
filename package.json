    "mocha": "^2",
    "mocha-lcov-reporter": "0.0.2",
    "proxyquire": "^1.0.1",
    "rimraf": "^2.2.8",
    "should": "~7",
    "sinon": "^1.9.1",
    "stream-assert": "^2.0.1",
    "strip-ansi": "^3.0.0"
  },
